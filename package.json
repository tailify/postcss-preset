{
  "name": "@tailify/postcss-preset",
  "version": "2.0.0",
  "description": "Tailify's very own opinionated PostCSS preset",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only",
    "test-only": "jest"
  },
  "repository": "tailify/postcss-preset",
  "keywords": [
    "postcss",
    "postcss-preset"
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/tailify/postcss-preset/issues",
  "homepage": "https://github.com/tailify/postcss-preset#readme",
  "dependencies": {
    "cssnano": "4.1.10",
    "postcss-preset-env": "6.6.0"
  },
  "devDependencies": {
    "@tailify/eslint-config-legacy": "^2.0.0",
    "@tailify/prettier-config": "^1.0.0",
    "eslint": "^6.7.0",
    "jest": "^24.7.0",
    "postcss": "^7.0.0",
    "postcss-load-config": "^2.0.0",
    "prettier": "^1.15.0"
  },
  "peerDependencies": {
    "postcss": "^7.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    },
    "prTitles": {
      "basicPR": "Update ${dependency} to the latest version",
      "groupPR": "Update ${dependency} in group ${group} to the latest version"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
